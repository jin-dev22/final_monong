<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.kh.monong.notice.model.dao.NotificationDao">
  <insert id="insertNotification">
  	insert into member_notification 
  		values(seq_noti_no.nextval, #{memberId}, #{notiContent}, default, default, 
  			<if test="inquireNo != null">
  				#{inquireNo},
  			</if>
  			<if test="dInquireNo != null">
  				#{dInquireNo},
  			</if>
  			<if test="dOrderNo != null">
  				#{dOrderNo},
  			</if>
  			<if test="sOrderNo != null">
  			  	#{sOrderNo},
  			</if>
  			#{messageType}
  		)
  </insert>
</mapper>